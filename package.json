{
  "name": "d2-charts-api",
  "version": "32.0.6",
  "description": "DHIS2 charts api",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rm -rf lib/",
    "build": "npm run localize && ./node_modules/.bin/babel src --copy-files -d lib/ --presets es2015,stage-2 --source-maps",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n NAMESPACE -p ./i18n/ -o ./src/locales/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@dhis2/d2-i18n": "^1.0.4",
    "@dhis2/d2-ui-analytics": "^1.0.5",
    "d2-utilizr": "0.2.13",
    "d3-color": "1.0.1",
    "highcharts": "^6.2.0",
    "highcharts-exporting": "^0.1.7",
    "highcharts-more": "^0.1.7",
    "highcharts-no-data-to-display": "^0.1.7",
    "highcharts-solid-gauge": "^0.1.7"
  },
  "devDependencies": {
    "@dhis2/d2-i18n-extract": "^1.0.8",
    "@dhis2/d2-i18n-generate": "^1.1.1",
    "babel-cli": "^6.3.15",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13"
  }
}
