{
  "name": "@dhis2/d2-charts-api",
  "version": "1.0.0",
  "description": "DHIS2 charts api",
  "main": "./build/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "rm -rf build/ && ./node_modules/.bin/babel src/ -d build/ --presets es2015,stage-2 --source-maps"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "d2-utilizr": "0.2.13",
    "d3-color": "1.0.1",
    "highcharts": "^6.2.0",
    "highcharts-exporting": "^0.1.7",
    "highcharts-more": "^0.1.7",
    "highcharts-no-data-to-display": "^0.1.7",
    "highcharts-solid-gauge": "^0.1.7"
  },
  "devDependencies": {
    "@dhis2/cli-style": "^3.3.4",
    "babel-cli": "^6.3.15",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "husky": "^2.4.1"
  }
}
